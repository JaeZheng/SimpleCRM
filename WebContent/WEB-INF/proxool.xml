<?xml version="1.0" encoding="UTF-8"?>
<something-else-entirely> 
<proxool>    
    <alias>crm</alias>    
     <driver-url>jdbc:mysql://localhost:3306/web_book?autoReconnect=true</driver-url> 
    <!--<driver-url>jdbc:microsoft:sqlserver://localhost:1433;databasename=kc;</driver-url>-->
    <driver-class>com.mysql.jdbc.Driver</driver-class>    
    <driver-properties>      
        <property name="user" value="root"/>      
        <property name="password" value="root"/>
        <property name="autoReconnect" value="true" />
    </driver-properties>
    <prototype-count>5</prototype-count>
    <maximum-connection-count>150</maximum-connection-count>
    <minimum-connection-count>5</minimum-connection-count>
    <maximum-new-connections>10</maximum-new-connections>
    <simultaneous-build-throttle>20</simultaneous-build-throttle>
    <test-before-use>true</test-before-use>
    <test-after-use>true</test-after-use>
    <maximum-active-time>60000</maximum-active-time>
    <house-keeping-sleep-time>90000</house-keeping-sleep-time>
    <house-keeping-test-sql>select CURRENT_DATE</house-keeping-test-sql>
    <trace>true</trace>
    <verbose>true</verbose>
</proxool>
</something-else-entirely>